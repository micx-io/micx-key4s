{
  "name": "micx/key4s",
  "description": "OpenID Connect Server",
  "license": "MIT",
  "homepage": "https://infracamp.org/project/phore",
  "authors": [
    { "name": "Matthias Leuffen", "email": "m@tth.es" },
    { "name": "Michael Schaar"}
  ],
  "repositories": [
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "brace/core": "*",
    "brace/dbg": "*",
    "brace/mod-session": "*",
    "brace/mod-cors": "*",
    "brace/mod-body": "*",
    "brace/mod-router": "*",
    "brace/mod-request-laminas": "*",
    "lack/oid-server-base": "*",
    "firebase/php-jwt": "v5.4.0",
    "phore/unidb": "*",
    "phore/hydrator": "*",
    "phore/misc": "*"
  },
  "require-dev": {
    "phore/tester": "*"
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "Micx\\Key4s\\": "src/"
    },
    "files": [
      "unidb.php"
    ]
  },
  "autoload-dev": {
    "psr-4": { "Phore\\Tests\\": "tests/" }
  },
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "source"
  }
}
